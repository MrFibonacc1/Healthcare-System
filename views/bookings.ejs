      .dropdown-content label {
          display: block;
          padding: 10px; /* Match padding to form-group */
          margin: 0;
      }
  
      .dropdown-content input[type="checkbox"] {
          margin-right: 5px;
      }
  
      .dropdown-content::before {
          content: '';
          position: absolute;
          top: -5px;
          left: 50%;
          transform: translateX(-50%);
          border-width: 0 5px 5px;
          border-style: solid;
          border-color: #ced4da transparent;
